
<div id="wrapper">
  <header class="navbar navbar-default navbar-fixed-top" role="navigation" id="navbar">
    <ul class="dropdown-menu dropdown-user">
      <li><a href="/logout"><i class="fa fa-sign-out fa-fw"></i>Logout</a>
      </li>
    </ul>
  </header>
  <main>
    <div class="user-info">
      <h4> {{email}}</h4>
      <h5> Member since {{date}}</h5>
      <div class="user-text-online">
          <span class="user-circle-online btn btn-success btn-circle "></span>&nbsp;Online
      </div>
    </div>
  <form>
    <div class="form-group row">
      <label for="inputTitle" class="col-sm-2 col-form-label">&nbsp;Title</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="inputTitle" placeholder="Title">
      </div>
    </div>
    <div class="form-group row">
      <label for="inputLink" class="col-sm-2 col-form-label">&nbsp;Link</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="inputLink" placeholder="http://">
      </div>
    </div>
    <div class="form-group row">
      <label for="inputTags" class="col-sm-2 col-form-label">&nbsp;Tags</label>
      <div class="col-sm-8">
        <input type="text" class="form-control" id="inputTags" placeholder="Tags">
      </div>
    </div>
    <div class="form-group row">
      <label for="difficulty" class="col-sm-2 col-form-label">&nbsp;Difficulty</label>
        <select class="form-control" id="difficulty">
          <option>1</option>
          <option>2</option>
          <option>3</option>
          <option>4</option>
          <option>5</option>
        </select>
    </div> 
  </form>
</main>
   

 

   
   

